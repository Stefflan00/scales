<div class="well">
  <h4>
    <%= @.id %>
    <small class="pull-right"><time class="timeago" datetime="<%= @.spawned_at %>"></time></small>
  </h4>
  <hr />
  <p class="machine">
    <span><i class="icon-eye-open"></i> <a href="http://<%= @.ip %>:<%= @.port %>"><%= @.ip %>:<%= @.port %></a></span>
    <span class="pull-right"><i class="icon-leaf"></i> <%= @.env %></span>
  </p>
</div>